! Cursor
Xcurson.theme : Vanilla-DMZ-AA

! urxvt color scheme:
URxvt*background: #2B2B2B
URxvt*foreground: #DEDEDE

URxvt*loginshell : true

URxvt*colorUL: #86a2b0


! Colors by Hex
! cyan
URxvt*color6: #06989A
URxvt*color14: #34E2E2
! blue
URxvt*color4 : #06989A
URxvt*color12 : #34E2E2
! black
URxvt*color0 : #2E3436
URxvt*color8 : #555753
! red
URxvt*color1 : #CC0000
URlxvt*color9 : #EF2929
! green
URxvt*color2 : #4E9A06
URxvt*color10 : #8AE234
! yellow 
URxvt*color3 : #C4A000
URxvt*color11 : #FCE94F
! magenta
URxvt*color5 : #75507B
URxvt*color13 : #AD7FA8
! white
URxvt*color7 : #D3D7CF
URxvt*color15 : #EEEEEC

! font
! Options: Monospace, terminus
URxvt*font : xft:terminus:pixelsize=12
URxvt*boldFont : xft:terminus:bold:pixelsize=12

! extension for links
URxvt.perl-ext-common : default, matcher

! enabling clickable links
URxvt.urlLauncher : /usr/bin/chromium
URxvt.matcher.button : 1

! Scrolling/Cursor Style
URxvt*saveLines : 12000
URxvt*scrollstyle : plain
URxvt*scrollBar : false
URxvt*cursonUnderline : true

! xterm settings

XTerm*background: black
XTerm*faceName: Bitstream Vera Serif Mono
XTerm*faceSize: 11
XTerm*background: #2B2B2B
XTerm*foreground: #DEDEDE
XTerm*cursorColor: #8FAF9F

